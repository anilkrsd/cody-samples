BEGIN:VCALENDAR
METHOD:PUBLISH
VERSION:2.0
X-WR-CALNAME:Cody
PRODID:-//Cody Inc//Cody Calendar//EN
X-APPLE-CALENDAR-COLOR:#00FFFF
X-WR-TIMEZONE:Europe/Brussels
CALSCALE:GREGORIAN
BEGIN:VTIMEZONE
TZID:Europe/Brussels 
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
DTSTART:19810329T020000
TZNAME:CEST
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZOFFSETFROM:+0200
TZNAME:CEST
DTSTART:19961027T030000
END:STANDARD
END:VTIMEZONE
<% for(var iE in events){ var event = events[iE]; %>
BEGIN:VEVENT
SUMMARY:<%=event.title%>
STATUS:CONFIRMED
DTSTART:<%=event.start%>
DTEND:<%=event.end%>
DTSTAMP: <%=event.timestamp%>Z
LOCATION:<%=event.location%>
UID:<%=event.id%>
DESCRIPTION:<%=event.description%>
END:VEVENT
<% } %>
END:VCALENDAR
